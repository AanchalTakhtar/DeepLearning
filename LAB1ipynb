   "source": [
    "#convert data to pytorch tensors\n",
    "x_train_tensor= torch.tensor(x_train,dtype=torch.float32)\n",
    "x_test_tensor= torch.tensor(x_test,dtype=torch.float32)\n",
    "y_train_tensor= torch.tensor(y_train.values,dtype=torch.float32).view(-1,1)\n",
    "y_test_tensor= torch.tensor(y_test.values, dtype=torch.float32).view(-1,1)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 35,
   "id": "bc3416ac-5e91-4fba-8114-a540b2b16a4f",
   "metadata": {},
   "outputs": [],
   "source": [
    "#define model\n",
    "model= nn.Sequential(\n",
    "    nn.Linear(x_train.shape[1],12),nn.ReLU(),\n",
    "    nn.Linear(12,8),nn.ReLU(),\n",
    "    nn.Linear(8,1),nn.Sigmoid()\n",
    ")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 36,
   "id": "5fd17c2b-a246-4345-9e1e-7231c361473a",
   "metadata": {},
   "outputs": [],
   "source": [
    "#train the model\n",
    "optimizer=optim.Adam(model.parameters(),lr=0.001)\n",
    "critreion= nn.BCELoss()\n",
    "\n",
    "for _ in range(10):\n",
    "    optimizer.zero_grad()\n",
    "    loss= critreion(model(x_train_tensor),y_train_tensor)\n",
    "    loss.backward()\n",
    "    optimizer.step()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 37,
   "id": "98f4aef4-a1d9-4a55-8cbf-cd2004920505",
   "metadata": {},
