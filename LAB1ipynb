   ],
   "source": [
    "# Evaluate the model\n",
    "with torch.no_grad():\n",
    "    torch_y_pred = (model(x_test_tensor) > 0.5).float()  # Make sure to cast the output to float\n",
    "    accuracy = (torch_y_pred.eq(y_test_tensor).sum().item()) / y_test_tensor.size(0)  # Calculate accuracy\n",
    "\n",
    "print(f'Accuracy: {accuracy}')\n"
   ]
